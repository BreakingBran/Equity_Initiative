<h1>Inspire!</h1>
<h3>HELLO THIS IS ALL THE USERS</h3>

<table>
	<thead>
		<tr>
			<td>First Name</td>
			<td>Last Name</td>
			<td>Username</td>
			<td>Password</td>
			<td>Bio</td>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
			<tr>
				<td><%= user.first_name %></td>
				<td><%= user.last_name %></td>
				<td><%= user.username %></td>
				<td><%= user.password %></td>
				<td><%= user.bio %></td>
			</tr>
		<% end %>
	</tbody>
</table>